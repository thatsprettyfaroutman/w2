    "babel-preset-react": "6.24.1",
    "echoascii": "1.0.3",
    "npm-run-all": "4.1.2",
    "parcel-bundler": "^1.9.6",
    "postcss-apply": "0.10.0",
    "postcss-browser-reporter": "0.5.0",
    "postcss-calc": "6.0.1",
